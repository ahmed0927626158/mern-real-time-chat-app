{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api",
      "methods":["POST","GET","PUT","DELETE","PATCH","OPTIONS"],
      "headers":{
        "Access-Control-Allow-Origin":"*"
        
      }
     
    }
  ]
}
